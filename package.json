{
  "name": "multiline-ts",
  "version": "2.2.0",
  "description": "Readable multiline template strings",
  "type": "module",
  "main": "./build/wrapper.cjs",
  "exports": {
    "import": "./build/code/index.js",
    "require": "./build/wrapper.cjs"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:peterjwest/multiline-ts.git"
  },
  "files": [
    "/build"
  ],
  "author": "Peter West",
  "bugs": {
    "url": "https://github.com/peterjwest/multiline-ts/issues"
  },
  "keywords": [
    "string",
    "multline",
    "dedent",
    "deindent",
    "es6"
  ],
  "license": "MIT",
  "homepage": "https://github.com/peterjwest/multiline-ts#readme",
  "scripts": {
    "audit-dependencies": "yarn audit; [[ $? -ge 4 ]] && exit 1 || exit 0",
    "build": "rm -rf build/code && tsup src/index.ts --format cjs,esm --dts --out-dir build/code",
    "lint": "eslint .",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "test:snippets": "test-snippets"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/coverage-c8": "^0.29.7",
    "eslint": "^8.36.0",
    "sinon": "^11.1.1",
    "test-snippets": "^0.3.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^4.3.2",
    "vitest": "^0.29.7"
  }
}
